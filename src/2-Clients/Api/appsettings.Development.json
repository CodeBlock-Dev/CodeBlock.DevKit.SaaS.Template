{
  "DetailedErrors": true,
  "MongoDB": {
    "DatabaseName": "CodeBlock_Dev_Dev"
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Optimization": {
    "EnableCaching": false
  },
  "Swagger": {
    "Enabled": true
  }
}
