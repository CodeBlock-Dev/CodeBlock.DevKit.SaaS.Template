{
  "AllowedHosts": "*",
  //This needs to be shared with the client application
  "ApiKey": "XXXXXXXXXXX",
  "Application": {
    "Default": {
      "Name": "Can Be Yours",
      "SupportEmail": "support@test.com",
      "LogoUrl": "/images/logos/logo.png",
      "FaviconUrl": "/favicon.ico",
      "Url": "https://api.canbeyours.online/"
    },
    "Localizations": {
      "fa": {
        "Name": "میتونه مال تو باشه!"
      },
      "es": {
        "Name": "¡Puede ser tuyo!"
      },
      "de": {
        "Name": "Kann dir gehören!"
      },
      "fr": {
        "Name": "Peut être à vous!"
      },
      "pt": {
        "Name": "Pode ser seu!"
      },
      "ar": {
        "Name": "يمكن أن يكون لك!"
      }
    }
  },
  "MongoDB": {
    "Connection": "mongodb://localhost:27017",
    "DatabaseName": "CanBeYours"
  },
  "Administration": {
    "AdminRole": "admin",
    "Permissions": [
      {
        "DisplayName": "Test",
        "SystemName": "Test",
        "GroupName": "Custom"
      }
    ]
  },
  "Identity": {
    "AdminUser": {
      "Mobile": "+31000000000",
      "Email": "admin@test.com",
      "Password": "12345678"
    },
    "ExternalLogins": {
      "Google": {
        "Enabled": true,
        "ClientId": "XXXXXXXXXXX",
        "ClientSecret": "XXXXXXXXXXX",
        "CallbackPath": "/signin-google"
      },
      "Twitter": {
        "Enabled": true,
        "ConsumerKey": "XXXXXXXXXXX",
        "ConsumerSecret": "XXXXXXXXXXX",
        "CallbackPath": "/signin-twitter"
      },
      "Microsoft": {
        "Enabled": true,
        "ClientId": "XXXXXXXXXXX",
        "ClientSecret": "XXXXXXXXXXX",
        "CallbackPath": "/signin-microsoft"
      },
      "Facebook": {
        "Enabled": true,
        "AppId": "XXXXXXXXXXX",
        "AppSecret": "XXXXXXXXXXX",
        "CallbackPath": "/signin-facebook"
      }
    }
  },
  "CookieAuthentication": {
    "Enabled": true,
    "CookieName": "CanBeYours.Api.AuthCookie",
    "CookieHttpOnly": true,
    "LoginPath": "/login",
    "LogoutPath": "/logout",
    "ExpireFromMinute": 120,
    "SlidingExpiration": false,
    "AllowRefresh": true
  },
  "Localization": {
    "CookieName": "CanBeYours.Api.Culture",
    "Languages": [
      {
        "Name": "English",
        "Code": "en",
        "ShortName": "En",
        "Direction": "ltr",
        "Font": "system-ui, -apple-system, \"Segoe UI\", Roboto, \"Helvetica Neue\", \"Noto Sans\", \"Liberation Sans\", Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\"",
        "IsDefault": true
      },
      {
        "Name": "Persian",
        "Code": "fa",
        "ShortName": "Fa",
        "Direction": "rtl",
        "Font": "Vazir"
      },
      {
        "Name": "Spanish",
        "Code": "es",
        "ShortName": "Es",
        "Direction": "ltr",
        "Font": "system-ui, -apple-system, \"Segoe UI\", Roboto, \"Helvetica Neue\", \"Noto Sans\", \"Liberation Sans\", Arial, sans-serif"
      },
      {
        "Name": "German",
        "Code": "de",
        "ShortName": "De",
        "Direction": "ltr",
        "Font": "system-ui, -apple-system, \"Segoe UI\", Roboto, \"Helvetica Neue\", \"Noto Sans\", \"Liberation Sans\", Arial, sans-serif"
      },
      {
        "Name": "French",
        "Code": "fr",
        "ShortName": "Fr",
        "Direction": "ltr",
        "Font": "system-ui, -apple-system, \"Segoe UI\", Roboto, \"Helvetica Neue\", \"Noto Sans\", \"Liberation Sans\", Arial, sans-serif"
      },
      {
        "Name": "Portuguese",
        "Code": "pt",
        "ShortName": "Pt",
        "Direction": "ltr",
        "Font": "system-ui, -apple-system, \"Segoe UI\", Roboto, \"Helvetica Neue\", \"Noto Sans\", \"Liberation Sans\", Arial, sans-serif"
      },
      {
        "Name": "Arabic",
        "Code": "ar",
        "ShortName": "Ar",
        "Direction": "rtl",
        "Font": "system-ui, \"Segoe UI\", \"Helvetica Neue\", Tahoma, Arial, sans-serif"
      }
    ]

  },
  "Caching": {
    "CacheTimeInSeconds": 10,
    "Enabled": true
  },
  "Monitoring": {
    "Service": {
      "Name": "CanBeYours.Api",
      "HealthCheckPath": "/health"
    },
    "Logging": {
      "IncludeFormattedMessage": true,
      "IncludeScopes": true,
      "ParseStateValues": true,
      "MinimumLogLevel": "Information",
      "Filters": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Hangfire.BackgroundJobServer": "Warning",
        "Hangfire.Server.BackgroundServerProcess": "Warning"
      }
    }
  },
  "Security": {
    "EncryptionSymmetricKey": "XXXXXXXXXXX",
    "SigningPrivateKeyPath": "GET_FROM_ENVIRONMENT_VARIABLE",
    "RateLimiter": {
      "Enabled": true,
      "PermitLimit": 100,
      "WindowSeconds": 60,
      "QueueLimit": 0,
      "QueueProcessingOrder": "OldestFirst"
    }
  },
  "AIChatBot": {
    "QdrantDB": {
      "Host": "localhost",
      "Port": 6334,
      "Https": false,
      "ApiKey": "XXXXXXXXXXX"
    }
  },
  "Swagger": {
    "Enabled": true,
    "Title": "Can Be Yours Api Documentation",
    "Version": "v1",
    "RedirectRootAddress": true
  },
  "JwtAuthentication": {
    "Key": "XXXXXXXXXXX",
    "Issuer": "https://api.canbeyours.online/",
    "ExpireDays": 30
  }
}